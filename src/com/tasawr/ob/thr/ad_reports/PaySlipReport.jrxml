	<queryString><![CDATA[select cl.name clientname,emp.documentno EmpNo,emp.known_as EmpName,dept.name department,desg.name designation,emp.date_of_joining DOJ,org.name Organization,emp.salary_amount grossamount,salpayment.otamt otamount,
salpayment.tot_earnings TotalEarnings,salpayment.tot_lopdays LOPDays,salpayment.tot_lopamt LOPAmount,salpayment.loanamt  LoanDeductions,tot_deductions TotalDeduction,salpayment.tot_paidamt NetPay,salpayment.daysworked NoofDaysWorked,salpayment.noofholidays Holidays,salpayment.leavedays Leaves, salpayment.noofweekends WeaklyOff,period.name "FortheMonthof",emp.accountno BankAccountNo
from thr_empl_info emp,thr_em_department dept,thr_em_designation desg ,ad_org org ,thr_empl_sal_perd_emp salpayment,c_period period ,c_year years,ad_client cl
where emp.thr_em_department_id = dept.thr_em_department_id
and emp.thr_em_designation_id = desg.thr_em_designation_id
and emp.ad_org_id = org.ad_org_id
and emp.thr_empl_info_id = salpayment.thr_empl_info_id
and years.c_year_id = period.c_year_id
and emp.ad_client_id=cl.ad_client_id
and salpayment.c_period_id = period.c_period_id
and (salpayment.ad_org_id = coalesce($P{AD_Org_ID},salpayment.AD_ORG_ID) or $P{AD_Org_ID}='0')
and salpayment.c_period_id = $P{C_Period_ID}
and (emp.documentno =coalesce($P{Documentno},emp.documentno) or $P{Documentno}='0')]]></queryString>

