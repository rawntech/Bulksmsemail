	<parameter name="AD_Org_ID" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<parameter name="Fromdate" isForPrompting="true" class="java.util.Date"/>
	<parameter name="Todate" isForPrompting="true" class="java.util.Date"/>
	<parameter name="Documenttype" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select cl.name clientname ,b.documentno as Employeeno,b.known_as as name,d.name as Nationality,e.name as Organization, 'Organization: ' ||e.name as title, 
case when doc.documenttype='NC' then 'National ID'
when doc.documenttype='PP' then 'Passport'
when doc.documenttype='RP' then 'Residential Permit'
when doc.documenttype='DL' then 'Drivers License'
when doc.documenttype='VS' then 'Visa'
when doc.documenttype='LC' then 'Labor Card'
when doc.documenttype='AG' then 'Agreements Paper'
when doc.documenttype='IN' then 'Insurance No'
when doc.documenttype='EM' then 'Emirates ID' end as documenttype,
doc.documentno as IDCardnumber,doc.documentexpirydate as Expirationdate
from thr_empl_document doc
left join thr_empl_info b on doc.thr_empl_info_id=b.thr_empl_info_id
left join thr_empl_personal c on b.thr_empl_info_id=c.thr_empl_info_id
left join c_country d on d.c_country_id=c.c_country_id
left join ad_org e on e.ad_org_id=b.ad_org_id
left join ad_client cl on cl.ad_client_id=doc.ad_client_id
where  (doc.AD_ORG_ID=coalesce($P{AD_Org_ID},doc.AD_ORG_ID) or $P{AD_Org_ID}='0')
and doc.documentexpirydate between $P{Fromdate} and $P{Todate}
and (doc.documenttype=coalesce($P{Documenttype},doc.documenttype) or $P{Documenttype}='0')
order by b.documentno, doc.documentexpirydate]]></queryString>

