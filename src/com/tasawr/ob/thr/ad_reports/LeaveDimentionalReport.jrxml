	<queryString><![CDATA[select cl.name clientname,emp.documentno Empno,emp.known_as empname,ltype.name leavetype,lvappl.fromdate,lvappl.todate,lvappl.requested_days,lvappl.description,org.name
	     from thr_empl_leave_type ltype,thr_em_lv_appl lvappl,thr_empl_info emp , ad_org org, ad_client cl
	     where lvappl.thr_empl_info_id = emp.thr_empl_info_id
	     and lvappl.thr_empl_leave_type_id= ltype.thr_empl_leave_type_id
	     and lvappl.ad_org_id = org.ad_org_id
	     and emp.ad_client_id=cl.ad_client_id
	     and (lvappl.ad_org_id = coalesce($P{AD_Org_ID},lvappl.AD_ORG_ID) or $P{AD_Org_ID}='0')
	     and lvappl.fromdate >= $P{FromDate}
	     and lvappl.todate <= $P{ToDate}
	     Order by emp.documentno]]></queryString>

