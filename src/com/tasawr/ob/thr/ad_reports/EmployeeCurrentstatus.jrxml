	<parameter name="AD_Org_ID" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<parameter name="Documenttype" isForPrompting="true" class="java.lang.String"/>
	<parameter name="Created" isForPrompting="true" class="java.util.Date"/>
	<queryString><![CDATA[select cl.name clientname,emp.documentno empid,emp.known_as employee,
dept.name department,
case when doc.documenttype='NC' then 'National ID'
when doc.documenttype='PP' then 'Passport'
when doc.documenttype='RP' then 'Residential Permit'
when doc.documenttype='DL' then 'Drivers License'
when doc.documenttype='VS' then 'Visa'
when doc.documenttype='LC' then 'Labor Card'
when doc.documenttype='AG' then 'Agreements Paper'
when doc.documenttype='IN' then 'Insurance No'
when doc.documenttype='EM' then 'Emirates ID' end as documenttype,
doc.documentno,doc.documentexpirydate,
desg.name designation,c.name countryname,emp.date_of_joining doj,
emp.status empstatus,org.name orgname
from thr_empl_info emp
LEFT JOIN thr_empl_document doc ON emp.thr_empl_info_id = doc.thr_empl_info_id
LEFT JOIN thr_empl_personal p ON emp.thr_empl_info_id = p.thr_empl_info_id
LEFT JOIN thr_em_department dept ON emp.thr_em_department_id = dept.thr_em_department_id 
LEFT JOIN thr_em_designation desg ON emp.thr_em_designation_id = desg.thr_em_designation_id 
LEFT JOIN c_country c ON p.c_country_id = c.c_country_id
LEFT JOIN ad_org org ON emp.ad_org_id=org.ad_org_id
LEFT JOIN ad_client cl ON emp.ad_client_id=cl.ad_client_id
where  (emp.ad_org_id =coalesce($P{AD_Org_ID},emp.AD_ORG_ID) or $P{AD_Org_ID}='0')
and doc.documenttype = $P{Documenttype}    
and emp.created <= $P{Created}
        
Order by empid]]></queryString>

