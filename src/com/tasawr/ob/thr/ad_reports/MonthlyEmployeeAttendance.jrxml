	<parameter name="AD_Org_ID" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<parameter name="Attendance_date" isForPrompting="true" class="java.util.Date"/>
	<parameter name="Attendance_todate" isForPrompting="true" class="java.util.Date"/>
	<queryString><![CDATA[select distinct emp.documentno,emp.known_as,dept.name department,desg.name designation,emp.date_of_joining,
DATE_PART('day', $P{Attendance_todate}::timestamp - $P{Attendance_date}::timestamp)+1 monthdays,
(DATE_PART('day', $P{Attendance_todate}::timestamp - $P{Attendance_date}::timestamp)+1) - thr_noofholidays_per($P{Attendance_date},$P{Attendance_todate}) Workingdays,
(DATE_PART('day', $P{Attendance_todate}::timestamp - $P{Attendance_date}::timestamp)+1)-(thr_noolop_per($P{Attendance_date},$P{Attendance_todate},att.thr_empl_info_id)+thr_penaltyday_per($P{Attendance_date},$P{Attendance_todate},att.thr_empl_info_id)+thr_noofabsent_per ($P{Attendance_date},$P{Attendance_todate},att.thr_empl_info_id)) payabledays,
thr_noofholidays_per($P{Attendance_date},$P{Attendance_todate}) holidays,
thr_noofpresent_per ($P{Attendance_date},$P{Attendance_todate},att.thr_empl_info_id) present,
thr_noofabsent_per ($P{Attendance_date},$P{Attendance_todate},att.thr_empl_info_id) Absent,
thr_noofsick_per($P{Attendance_date},$P{Attendance_todate},att.thr_empl_info_id) sick,
thr_noofother_per($P{Attendance_date},$P{Attendance_todate},att.thr_empl_info_id) otherleave,
thr_onepunch_per($P{Attendance_date},$P{Attendance_todate},att.thr_empl_info_id) One_Punch_no,
thr_penaltyday_per($P{Attendance_date},$P{Attendance_todate},att.thr_empl_info_id) PenaltyDay,
thr_noolop_per($P{Attendance_date},$P{Attendance_todate},att.thr_empl_info_id) LOP,
sum(late) Total_Late_Min,sum(early_min) Total_Early_Min,
sum(early_left) Total_Early_Left_Min,sum(comming_early) Coming_Early,sum(staying_late) Staying_Late,
org.name organization ,sum(att.over_time) OT
from thr_empl_info emp,thr_em_department dept,thr_em_designation desg ,
ad_org org ,thr_em_attn_proc att
where emp.thr_empl_info_id = att.thr_empl_info_id
and emp.thr_em_department_id = dept.thr_em_department_id
and emp.thr_em_designation_id = desg.thr_em_designation_id
and att.attendance_date >= $P{Attendance_date} and att.attendance_date <= $P{Attendance_todate}
and emp.ad_org_id = org.ad_org_id
and (att.ad_org_id =coalesce($P{AD_Org_ID},att.AD_ORG_ID) or $P{AD_Org_ID}='0')
group by emp.documentno,emp.known_as,dept.name,desg.name,monthdays,organization,att.thr_empl_info_id,emp.date_of_joining --,month.name 
order by emp.documentno,dept.name]]></queryString>

